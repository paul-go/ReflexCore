{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug Reflex Core Makets Script",
			"type": "node",
			"request": "launch",
			"program": "${env:HOME}/.npm-packages/bin/makets",
			"cwd": "${workspaceFolder}/ReflexCore",
			"args": [
				"publish"
			]
		},
		{
			"name": "Debug Reflex ML From Browser",
			"type": "chrome",
			"request": "launch",
			"url": "http://127.0.0.1:10001/??${workspaceFolder}/ReflexML/coverage",
			"webRoot": "${workspaceRoot}",
			"timeout": 30000,
			"smartStep": true,
			"sourceMaps": true
		},
		{
			"name": "Debug Reflex ML Makets Script",
			"type": "node",
			"request": "launch",
			"program": "${env:HOME}/.npm-packages/bin/makets",
			"cwd": "${workspaceFolder}/ReflexML",
			"args": [
				"publish"
			]
		},
		{
			"name": "Debug Reflex ML Node Makets Script",
			"type": "node",
			"request": "launch",
			"program": "${env:HOME}/.npm-packages/bin/makets",
			"cwd": "${workspaceFolder}/ReflexMLNode",
			"args": [
				"publish"
			]
		},
		{
			"name": "Debug Reflex SS Coverage",
			"type": "chrome",
			"request": "launch",
			"url": "http://127.0.0.1:7071",
			"webRoot": "${workspaceRoot}",
			"timeout": 30000,
			"smartStep": true,
			"sourceMaps": true
		},
		{
			"name": "Debug Reflex SS Emitter",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/ReflexSS.Emitter/build/reflex-ss-emitter.js",
			"args": [
				"./ReflexSS/core"
			],
			"smartStep": true,
			"sourceMaps": true
		}
	]
}
