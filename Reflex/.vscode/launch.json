{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Debug Reflex ML From Browser",
			"type": "chrome",
			"request": "launch",
			"url": "http://127.0.0.1:4349/ReflexML.Test/index.html",
			"webRoot": "${workspaceRoot}",
			"timeout": 30000,
			"smartStep": true,
			"sourceMaps": true
		},
		{
			"name": "Debug Reflex Core Makets Script",
			"type": "node",
			"request": "launch",
			"program": "${env:HOME}/.npm-packages/bin/makets",
			"cwd": "${workspaceFolder}/ReflexCore",
			"args": [
				"publish"
			]
		},
		{
			"name": "Debug Reflex ML Makets Script",
			"type": "node",
			"request": "launch",
			"program": "${env:HOME}/.npm-packages/bin/makets",
			"cwd": "${workspaceFolder}/ReflexML",
			"args": [
				"publish"
			]
		},
		{
			"name": "Debug Reflex ML Node Makets Script",
			"type": "node",
			"request": "launch",
			"program": "${env:HOME}/.npm-packages/bin/makets",
			"cwd": "${workspaceFolder}/ReflexMLNode",
			"args": [
				"publish"
			]
		},
		{
			"name": "Debug Reflex SS",
			"type": "chrome",
			"request": "launch",
			"url": "http://127.0.0.1:4349/ReflexSS.Test/index.html",
			"webRoot": "${workspaceRoot}",
			"timeout": 30000,
			"smartStep": true,
			"sourceMaps": true
		}
	]
}
